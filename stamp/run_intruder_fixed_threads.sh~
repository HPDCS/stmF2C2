#!/bin/bash

starting=1
ending=4
while [ $starting -le $ending ] 
	do
	echo "$starting per class $starting threads" >> original.txt
	echo " " >> original.txt
	echo ./intruder/intruder -a10 -l128 -n262144 -s1 -1 $starting -2 $starting -3 $starting -w 0 -y 1000000 -j 4000 -z 32 -t$starting 
	nice -20 ./intruder/intruder -a10 -l128 -n262144 -s1 -1 $starting -2 $starting -3 $starting -w 0 -y 1000000 -j 4000 -z 32 -t$starting >> original.txt
	echo " " >> original.txt
	starting=$[$starting+1]
done
